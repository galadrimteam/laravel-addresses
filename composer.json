{
    "name": "galadrim/laravel-addresses",
    "description": "Simple address and contact management for Laravel 5.",
    "homepage" : "https://github.com/AlexanderPoellmann/laravel-addresses",
    "license": "MIT",
    "keywords": [
        "addresses",
        "contacts",
        "laravel"
    ],
    "authors": [
        {
            "name" : "Thomas Hallot",
            "email": "thomas@galadrim.fr"
        }
    ],
    "require": {
        "php"                 : ">=5.5.9",
        "doctrine/dbal"       : "2.5.x",
        "illuminate/support"  : ">=5.1",
        "illuminate/database" : ">=5.1",
        "webpatser/laravel-countries" : "1.4.*"
    },
    "require-dev": {
        "mockery/mockery" : "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "vendocrat\\Addresses\\" : "src/"
        }
    },
    "extra": {
        "branch-alias" : {
            "dev-master" : "0.0.x-dev"
        }
    },
    "support" : {
        "source" : "https://github.com/AlexanderPoellmann/laravel-addresses",
        "issues" : "https://github.com/AlexanderPoellmann/laravel-addresses/issues"
    }
}
